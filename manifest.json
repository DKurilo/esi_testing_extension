{
  "name": "ESI testing extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Extension allows to debug ESI sites",
  "homepage_url": "https://github.com/DKurilo/esi-chrome-extension",
  "applications": {
    "gecko": {
      "id": "esi-testing-extension@kurilo.us"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["src/bg/background.js"]
  },
  "default_locale": "en",
  "permissions": [
    "activeTab","<all_urls>","webRequest","webRequestBlocking","webNavigation"
  ]
}